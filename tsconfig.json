{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "sourceMap": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "jsx": "react"
  },
  "formatCodeOptions": {
    "indentSize": 4,
    "tabSize": 4,
    "newLineCharacter": "\r\n",
    "convertTabsToSpaces": true,
    "insertSpaceAfterCommaDelimiter": true,
    "insertSpaceAfterSemicolonInForStatements": true,
    "insertSpaceBeforeAndAfterBinaryOperators": true,
    "insertSpaceAfterKeywordsInControlFlowStatements": true,
    "insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
    "insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
    "placeOpenBraceOnNewLineForFunctions": true,
    "placeOpenBraceOnNewLineForControlBlocks": true
  },
  "compileOnSave": true,
  "filesGlob": [
    "./*.ts",
    "./**/*.ts",
    "./*.tsx",
    "./**/*.tsx",
    "!./node_modules/**/*.ts",
    "!./bower_components/**/*.ts",
    "!./typings/**/*.ts",
    "!./node_modules/**/*.tsx",
    "!./bower_components/**/*.tsx",
    "!./typings/**/*.tsx"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "./app.ts",
    "./public/scripts/actions/TodoACtions.ts",
    "./public/scripts/reducers/TodoApp.ts",
    "./public/scripts/stores/TodoStore.ts",
    "./routes/Todo.ts",
    "./Scripts/typings/body-parser/body-parser.d.ts",
    "./Scripts/typings/compression/compression.d.ts",
    "./Scripts/typings/cookie-parser/cookie-parser.d.ts",
    "./Scripts/typings/debug/debug.d.ts",
    "./Scripts/typings/express-session/express-session.d.ts",
    "./Scripts/typings/express/express.d.ts",
    "./Scripts/typings/gulp-concat/gulp-concat.d.ts",
    "./Scripts/typings/gulp/gulp.d.ts",
    "./Scripts/typings/main-bower-files/main-bower-files.d.ts",
    "./Scripts/typings/method-override/method-override.d.ts",
    "./Scripts/typings/mime/mime.d.ts",
    "./Scripts/typings/morgan/morgan.d.ts",
    "./Scripts/typings/multer/multer.d.ts",
    "./Scripts/typings/node-uuid/node-uuid-base.d.ts",
    "./Scripts/typings/node-uuid/node-uuid-cjs.d.ts",
    "./Scripts/typings/node-uuid/node-uuid.d.ts",
    "./Scripts/typings/node/node.d.ts",
    "./Scripts/typings/orchestrator/orchestrator.d.ts",
    "./Scripts/typings/q/Q.d.ts",
    "./Scripts/typings/react-redux/react-redux.d.ts",
    "./Scripts/typings/react/react-dom.d.ts",
    "./Scripts/typings/react/react.d.ts",
    "./Scripts/typings/redux/redux.d.ts",
    "./Scripts/typings/serve-favicon/serve-favicon.d.ts",
    "./Scripts/typings/serve-static/serve-static.d.ts",
    "./Scripts/typings/superagent/superagent.d.ts",
    "./Scripts/typings/tsd.d.ts",
    "./public/scripts/components/AddTodo.tsx",
    "./public/scripts/components/Footer.tsx",
    "./public/scripts/components/Todo.tsx",
    "./public/scripts/components/TodoList.tsx",
    "./public/scripts/containers/TodoApp.tsx",
    "./public/scripts/pages/TodoIndex.tsx",
    "./views/error.tsx",
    "./views/Todo.tsx"
  ]
}